<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--本文件定义视觉规范中的颜色 均以main开头-->
    <!--titleBar背景色-->
    <color name="main_titlebar_background">#ffffff</color>
    <!--正常背景色-->
    <color name="main_background">#f0f0f0</color>
    <!--黑按钮背景-->
    <color name="main_black_btn">#666666</color>
    <!--辅助字色-->
    <color name="main_title">#333333</color>
    <!--主色 按钮色 红-->
